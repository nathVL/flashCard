{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}



{% block body %}

    <div class="logout">
        <p>Bienvenue {{ user.userIdentifier }} !</p>
        <a href="{{ path('app_logout') }}">Me d√©connecter</a>
